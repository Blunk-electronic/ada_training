with "gtkada.gpr";

 project gtkada_2 is

	--for exec_dir use external ("HOME") & "/bin";
	for exec_dir use ".";
 	for object_dir use "obj";
 	--for source_dirs use ("." , "src" , "src/include");
	--for source_dirs use (".");

 	for main use ("gtkada_2");
 end gtkada_2;

