with "gtkada.gpr";

 project gtkada_3 is

	--for exec_dir use external ("HOME") & "/bin";
	for exec_dir use ".";
 	for object_dir use "obj";
 	for source_dirs use ("." , "../../lib");
	--for source_dirs use (".");

 	for main use ("gtkada_3");
 end gtkada_3;

-- build with command "gprbuild"
-- clean up with command "gprclean"
